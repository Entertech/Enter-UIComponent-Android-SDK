        <attr name="rbs_mainColor" format="color"/>
        <attr name="rbs_textColor" format="color"/>
        <attr name="rbs_background" format="color|reference"/>
        <attr name="rbs_isShowInfoIcon" format="boolean"/>
        <attr name="rbs_isAbsoluteTimeAxis" format="boolean"/>
        <attr name="rbs_sample" format="integer"/>
        <attr name="rbs_spectrumColors" format="string"/>
        <attr name="rbs_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="ReportHeartRateView">

        <attr name="rhr_mainColor" format="color"/>
        <attr name="rhr_textColor" format="color"/>
        <attr name="rhr_background" format="color|reference"/>
        <attr name="rhr_isShowInfoIcon" format="boolean"/>
        <attr name="rhr_isAbsoluteTimeAxis" format="boolean"/>
        <attr name="rhr_sample" format="integer"/>
        <attr name="rhr_isShowAvg" format="boolean"/>
        <attr name="rhr_isShowMax" format="boolean"/>
        <attr name="rhr_isShowMin" format="boolean"/>
        <attr name="rhr_heartRateHighLineColor" format="color"/>
        <attr name="rhr_heartRateNormalLineColor" format="color"/>
        <attr name="rhr_heartRateLowLineColor" format="color"/>
        <attr name="rhr_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="ReportHeartRateVariabilityView">

        <attr name="hrv_mainColor" format="color"/>
        <attr name="hrv_textColor" format="color"/>
        <attr name="hrv_background" format="color|reference"/>
        <attr name="hrv_isShowInfoIcon" format="boolean"/>
        <attr name="hrv_isAbsoluteTimeAxis" format="boolean"/>
        <attr name="hrv_sample" format="integer"/>
        <attr name="hrv_lineColor" format="color"/>
        <attr name="hrv_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="ReportAffectiveView">

        <attr name="ra_mainColor" format="color"/>
        <attr name="ra_textColor" format="color"/>
        <attr name="ra_background" format="color|reference"/>
        <attr name="ra_isShowInfoIcon" format="boolean"/>
        <attr name="ra_isAbsoluteTimeAxis" format="boolean"/>
        <attr name="ra_sample" format="integer"/>
        <attr name="ra_isShowAvg" format="boolean"/>
        <attr name="ra_isShowMax" format="boolean"/>
        <attr name="ra_isShowMin" format="boolean"/>
        <attr name="ra_fillColor" format="color"/>
        <attr name="ra_emotionType" format="enum">
            <enum name="attention" value="0"/>
            <enum name="relaxation" value="1"/>
        </attr>

        <attr name="ra_infoUrl" format="string"/>
    </declare-styleable>
    <declare-styleable name="ReportPressureView">

        <attr name="rp_mainColor" format="color"/>
        <attr name="rp_textColor" format="color"/>
        <attr name="rp_background" format="color|reference"/>
        <attr name="rp_isShowInfoIcon" format="boolean"/>
        <attr name="rp_isAbsoluteTimeAxis" format="boolean"/>
        <attr name="rp_sample" format="integer"/>
        <attr name="rp_color" format="color"/>
        <attr name="rp_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomVerticalTextView">
        <attr name="cvtv_text" format="string"/>
        <attr name="cvtv_textSize" format="dimension"/>
        <attr name="cvtv_textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ChartLegendView">
        <attr name="clv_legend" format="string"/>
        <attr name="clv_color" format="color"/>
    </declare-styleable>

