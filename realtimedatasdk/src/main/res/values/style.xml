<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BrainWaveSurfaceView">
        <attr name="bwsf_lineColor" format="color"/>
        <attr name="bwsf_lineWidth" format="dimension"/>
        <attr name="bwsf_yAxisColor" format="color"/>
        <attr name="bwsf_gridLineColor" format="color"/>
        <attr name="bwsf_gridLineCount" format="integer"/>
        <attr name="bwsf_leftPadding" format="dimension"/>
        <attr name="bwsf_rightPadding" format="dimension"/>
        <attr name="bwsf_bgColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="EmotionIndicatorView">
        <attr name="eiv_textColor" format="color"/>
        <attr name="eiv_textSize" format="dimension"/>
        <attr name="eiv_textBottomPadding" format="dimension"/>
        <attr name="eiv_barWidth" format="dimension"/>
        <attr name="eiv_indicatorColor" format="color"/>
        <attr name="eiv_indicatorTopPadding" format="dimension"/>
        <attr name="eiv_leftPadding" format="dimension"/>
        <attr name="eiv_rightPadding" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="PercentProgressBar">
        <attr name="ppb_labelText" format="string"/>
        <attr name="ppb_labelTextColor" format="color"/>
        <attr name="ppb_labelTextSize" format="dimension"/>
        <attr name="ppb_percent" format="float"/>
        <attr name="ppb_percentTextColor" format="color"/>
        <attr name="ppb_percentTextSize" format="dimension"/>
        <attr name="ppb_barColor" format="color"/>
        <attr name="ppb_barWidth" format="dimension"/>
        <attr name="ppb_barLeftPadding" format="dimension"/>
        <attr name="ppb_barRightPadding" format="dimension"/>
        <attr name="ppb_leftPadding" format="dimension"/>
        <attr name="ppb_rightPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RealtimeHeartRateView">
        <attr name="rhrv_background" format="reference|color"/>
        <attr name="rhrv_isShowExtremeValue" format="boolean"/>
        <attr name="rhrv_mainColor" format="color"/>
        <attr name="rhrv_textColor" format="color"/>
        <attr name="rhrv_textFont" format="string"/>
        <attr name="rhrv_isShowInfoIcon" format="boolean"/>
        <attr name="rhrv_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="RealtimeAttentionView">
        <attr name="rav_background" format="reference|color"/>
        <attr name="rav_mainColor" format="color"/>
        <attr name="rav_textColor" format="color"/>
        <attr name="rav_textFont" format="string"/>
        <attr name="rav_isShowInfoIcon" format="boolean"/>
        <attr name="rav_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="RealtimeRelaxationView">
        <attr name="rrv_background" format="reference|color"/>
        <attr name="rrv_mainColor" format="color"/>
        <attr name="rrv_textColor" format="color"/>
        <attr name="rrv_textFont" format="string"/>
        <attr name="rrv_isShowInfoIcon" format="boolean"/>
        <attr name="rrv_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="RealtimePressureView">
        <attr name="rpv_background" format="reference|color"/>
        <attr name="rpv_mainColor" format="color"/>
        <attr name="rpv_textColor" format="color"/>
        <attr name="rpv_textFont" format="string"/>
        <attr name="rpv_isShowInfoIcon" format="boolean"/>
        <attr name="rpv_infoUrl" format="string"/>
    </declare-styleable>

    <declare-styleable name="RealtimeBrainwaveView">
        <attr name="rbv_background" format="reference|color"/>
        <attr name="rbv_mainColor" format="color"/>
        <attr name="rbv_textColor" format="color"/>
        <attr name="rbv_textFont" format="string"/>
        <attr name="rbv_isShowInfoIcon" format="boolean"/>
        <attr name="rbv_infoUrl" format="string"/>
        <attr name="rbv_leftBrainwaveColor" format="color"/>
        <attr name="rbv_rightBrainwaveColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="RealtimeBrainwaveSpectrumView">
        <attr name="rbsv_background" format="reference|color"/>
        <attr name="rbsv_mainColor" format="color"/>
        <attr name="rbsv_textColor" format="color"/>
        <attr name="rbsv_textFont" format="string"/>
        <attr name="rbsv_isShowInfoIcon" format="boolean"/>
        <attr name="rbsv_infoUrl" format="string"/>
        <attr name="rbsv_processBarColors" format="string"/>
    </declare-styleable>


    <declare-styleable name="StackedAreaChart">
        <attr name="sac_backgroundColor" format="color"/>
        <attr name="sac_gridLineColor" format="color"/>
        <attr name="sac_timeStampTextColor" format="color"/>
        <attr name="sac_timeStampTextSize" format="dimension"/>
        <attr name="sac_timeStampTextPaddingTop" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="HeartRateChart">

        <attr name="hrc_backgroundColor" format="color"/>
        <attr name="hrc_gridLineColor" format="color"/>
        <attr name="hrc_timeStampTextColor" format="color"/>
        <attr name="hrc_timeStampTextSize" format="dimension"/>
        <attr name="hrc_timeStampTextPaddingTop" format="dimension"/>
        <attr name="hrc_leftBarWidth" format="dimension"/>
        <attr name="hrc_leftBarTextSize" format="dimension"/>
        <attr name="hrc_leftBarTextPadding" format="dimension"/>
        <attr name="hrc_lowHeartRateColor" format="color"/>
        <attr name="hrc_neutralHeartRateColor" format="color"/>
        <attr name="hrc_highHeartRateColor" format="color"/>
        <attr name="hrc_curveLineWidth" format="dimension"/>
        <attr name="hrc_maxHeartRate" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PressureChart">
        <attr name="pc_backgroundColor" format="color"/>
        <attr name="pc_baseColor" format="string"/>
        <attr name="pc_gridLineLength" format="dimension"/>
        <attr name="pc_gridLineColor" format="color"/>
        <attr name="pc_gridLineWidth" format="dimension"/>
        <attr name="pc_timestampTextSize" format="dimension"/>
        <attr name="pc_timestampTextColor" format="color"/>
        <attr name="pc_timestampTextPaddingTop" format="color"/>
    </declare-styleable>

